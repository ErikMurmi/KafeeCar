<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/AVnombre1_txt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/administrarvendedores"
    android:backgroundTint="#FFFFFF">

    <LinearLayout
        android:id="@+id/administrar_vendedor_lyt"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/administrarvendedores"
        android:orientation="vertical"
        android:visibility="visible"
        tools:visibility="visible">

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toTopOf="parent"
            tools:layout_editor_absoluteX="0dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="65dp"
                android:layout_marginRight="25dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/busquedaCedulaVendedor_etxt2"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bordesbotongris"
                    android:drawableRight="@drawable/search_1"
                    android:ems="10"
                    android:fontFamily="@font/monglos"
                    android:inputType="numberSigned"
                    android:text="BÚSQUEDA"
                    android:textAlignment="center"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                    android:textColor="#959898"
                    android:textSize="15sp" />

                <Space
                    android:layout_width="wrap_content"
                    android:layout_height="25dp" />

                <LinearLayout
                    android:id="@+id/AVvendedor_lyt"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bordesmarcosnegro"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/AVimagenPerfil1_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:srcCompat="@tools:sample/avatars" />

                        <Space
                            android:layout_width="20dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="193dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/AVnombre_txt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="13dp"
                                android:fontFamily="@font/ocraext"
                                android:hint="Martha Pérez"
                                android:textAlignment="center"
                                android:textSize="21sp" />

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="15dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AVentrada_txt"
                                    android:layout_width="95dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Entrada:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraEntrada_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="8:00 am"
                                    android:textSize="17sp" />
                            </LinearLayout>

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="4dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AValmuerzo_txt"
                                    android:layout_width="106dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Almuerzo:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraAlmuerzo_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="13:00 pm"
                                    android:textSize="17sp" />
                            </LinearLayout>

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="4dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AVsalida_txt"
                                    android:layout_width="85dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Salida:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraSalida_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="17:00 pm"
                                    android:textSize="17sp" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="12dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/bordesmarcosnegro"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/AVvendedor2_lyt"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/AVimagenPerfil2_img"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            tools:srcCompat="@tools:sample/avatars" />

                        <Space
                            android:layout_width="20dp"
                            android:layout_height="wrap_content" />

                        <LinearLayout
                            android:layout_width="193dp"
                            android:layout_height="match_parent"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="13dp"
                                android:fontFamily="@font/ocraext"
                                android:hint="Martha Pérez"
                                android:textAlignment="center"
                                android:textSize="21sp" />

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="15dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AVentrada1_txt"
                                    android:layout_width="95dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Entrada:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraEntrada1_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="8:00 am"
                                    android:textSize="17sp" />
                            </LinearLayout>

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="4dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AValmuerzo1_txt"
                                    android:layout_width="106dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Almuerzo:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraAlmuerzo1_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="13:00 pm"
                                    android:textSize="17sp" />
                            </LinearLayout>

                            <Space
                                android:layout_width="match_parent"
                                android:layout_height="4dp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/AVsalida1_txt"
                                    android:layout_width="85dp"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="Salida:"
                                    android:textSize="17sp" />

                                <TextView
                                    android:id="@+id/AVhoraSalida1_txt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/ocraext"
                                    android:hint="17:00 pm"
                                    android:textSize="17sp" />
                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

                <Button
                    android:id="@+id/boton_ver_vendedor_btn"
                    android:layout_width="165dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="100dp"
                    android:layout_marginTop="50dp"
                    android:background="@drawable/boton_redondeado"
                    android:fontFamily="@font/valorant"
                    android:text="Ver"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="18sp" />

            </LinearLayout>
        </ScrollView>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>